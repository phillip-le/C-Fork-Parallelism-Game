#include "errors.h"

int main(int argc, char** argv) {
    if (argc != 4) {
        return player_error_msg(PLAYER_ARGS);
    }

    int playerCount = argv[2], playerID = argv[3];
    if (playerCount < 1) {
        return player_error_msg(PLAYER_COUNT);
    }
    
    if (playerID < 0 || playerID >= playerCount) {
        return player_error_msg(PLAYER_ID);
    }

    
    return PLAYER_OK;
}